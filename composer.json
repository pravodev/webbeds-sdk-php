{
    "name": "pravodev/hotel-api-sdk-php",
    "description": "WebBeds Hotel-API SDK for PHP",
    "type": "library",
    "require": {
        "php": ">= 7.2",
        "laminas/laminas-http": "^2.1",
        "laminas/laminas-json": "^3.1",
        "nicmart/string-template": "v0.1.0",
        "laminas/laminas-config": "^3.3"
    },
    "require-dev": {
        "php": ">= 7.2",
        "phpunit/phpunit": "7.1",
        "imsop/simplexml_debug": "^1.0"
    },
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Hamilton Wong",
            "email": "hamilton@aiart.io"
        },
        {
            "name": "Tomeu Capo",
            "email": "t.capo@hotelbeds.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "Webbeds\\HotelApiSdk\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Webbeds\\HotelApiSdk\\Tests\\": "tests/" }
    }
}
